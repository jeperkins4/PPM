                    <label class='accessibility' for='facility_facility_id'>Select A Facility to Manage</label>
                    <%= f.select(:facility_id, Facility.find(:all).collect {|fa| [ fa.facility, fa.id ] }.insert(0, ['All', -1]) , { :include_blank => true, :selected => (session[:facility]) ? session[:facility].id : '' },{:onchange => "JavaScript:submit()"}) -%>
                    <%= f.text_field :return_page, :value => @page,  :style => 'display:none;' %>
                  <% end -%>
                </div>
              <% else -%> 
                <div id='search'>
                  <label class='accessibility'>Managing:</label>
                  <%= session[:facility].facility %>
                </div>
              <% end -%>
          <% end -%>
            
              <h1><img align="ABSMIDDLE" src= "/images/palm_left.jpg" width="40" height="75"/>
