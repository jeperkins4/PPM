<h1>New incident_investigator</h1>

<%= error_messages_for :incident_investigator %>

<% form_for(:incident_investigator, :url => incident_investigators_path) do |f| %>
  <p>
    <b>Investigator</b><br />
    <%= f.text_field :investigator_id %>
  </p>

  <p>
    <b>Incident</b><br />
    <%= f.text_field :incident_id %>
  </p>

  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', incident_investigators_path %>