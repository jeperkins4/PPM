<h1>Editing position_archive</h1>

<%= error_messages_for :position_archive %>

<% form_for(:position_archive, :url => position_archive_path(@position_archive), :html => { :method => :put }) do |f| %>
  <p>
    <b>Position</b><br />
    <%= f.text_field :position_id %>
  </p>

  <p>
    <b>Filled date</b><br />
    <%= f.date_select :filled_date %>
  </p>

  <p>
    <b>Vacant date</b><br />
    <%= f.date_select :vacant_date %>
  </p>

  <p>
    <b>Employee id exit</b><br />
    <%= f.text_field :employee_id_exit %>
  </p>

  <p>
    <b>Employee id start</b><br />
    <%= f.text_field :employee_id_start %>
  </p>

  <p>
    <%= submit_tag "Update" %>
  </p>
<% end %>

<%= link_to 'Show', position_archive_path(@position_archive) %> |
<%= link_to 'Back', position_archives_path %>