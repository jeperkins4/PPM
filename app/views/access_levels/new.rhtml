<h1>New access_level</h1>

<%= error_messages_for :access_level %>

<% form_for(:access_level, :url => access_levels_path) do |f| %>
  <p>
    <b>Access level</b><br />
    <%= f.text_field :access_level %>
  </p>

  <p>
    <b>Description</b><br />
    <%= f.text_area :description %>
  </p>

  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', access_levels_path %>